# +mkmake+ -- Everything after this line is automatically generated

EXECUTABLES = c1 l1 o1 o2 o3 r1 r2 simpcat w1 w2

all: $(EXECUTABLES)

clean:
	rm -f core *.o $(EXECUTABLES) a.out 

.SUFFIXES: .c .o
.c.o:
	$(CC) $(CFLAGS) -c $*.c


c1: c1.c
	$(CC) $(CFLAGS) -o c1 c1.c

l1: l1.c
	$(CC) $(CFLAGS) -o l1 l1.c

o1: o1.c
	$(CC) $(CFLAGS) -o o1 o1.c

o2: o2.c
	$(CC) $(CFLAGS) -o o2 o2.c

o3: o3.c
	$(CC) $(CFLAGS) -o o3 o3.c

r1: r1.c
	$(CC) $(CFLAGS) -o r1 r1.c

r2: r2.c
	$(CC) $(CFLAGS) -o r2 r2.c

simpcat: simpcat.c
	$(CC) $(CFLAGS) -o simpcat simpcat.c

w1: w1.c
	$(CC) $(CFLAGS) -o w1 w1.c

w2: w2.c
	$(CC) $(CFLAGS) -o w2 w2.c
