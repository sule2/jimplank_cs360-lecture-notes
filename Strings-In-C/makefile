# +mkmake+ -- Everything after this line is automatically generated

EXECUTABLES = strcat strcat2 strchr strchr2 strcpy3 strcpy strcpy2 strlen makej strcat3 strcat4 sscanf1 scanf1 scanf2 scanf3

all: $(EXECUTABLES)

clean:
	rm -f core *.o $(EXECUTABLES) a.out

.SUFFIXES: .c .c
.c.c:
	$(CC) $(CFLAGS) -c $*.c

makej: makej.cpp
	g++ -o makej makej.cpp

quiz: quiz.c
	$(CC) $(CFLAGS) -o quiz quiz.c

strcat: strcat.c
	$(CC) $(CFLAGS) -o strcat strcat.c

strcat2: strcat2.c
	$(CC) $(CFLAGS) -o strcat2 strcat2.c

strcat4: strcat4.c
	$(CC) $(CFLAGS) -o strcat4 strcat4.c

strcat3: strcat3.c
	$(CC) $(CFLAGS) -o strcat3 strcat3.c

strchr: strchr.c
	$(CC) $(CFLAGS) -o strchr strchr.c

strchr2: strchr2.c
	$(CC) $(CFLAGS) -o strchr2 strchr2.c

strcpy: strcpy.c
	$(CC) $(CFLAGS) -o strcpy strcpy.c

strcpy2: strcpy2.c
	$(CC) $(CFLAGS) -o strcpy2 strcpy2.c

strcpy3: strcpy3.c
	$(CC) $(CFLAGS) -o strcpy3 strcpy3.c

strlen: strlen.c
	$(CC) $(CFLAGS) -o strlen strlen.c

sscanf1: sscanf1.c
	$(CC) $(CFLAGS) -o sscanf1 sscanf1.c

scanf1: scanf1.c
	$(CC) $(CFLAGS) -o scanf1 scanf1.c

scanf2: scanf2.c
	$(CC) $(CFLAGS) -o scanf2 scanf2.c

scanf3: scanf3.c
	$(CC) $(CFLAGS) -o scanf3 scanf3.c
